      "be27fde0-613f-4a3a-ba81-f826ca6fc91d": {
        "id": "be27fde0-613f-4a3a-ba81-f826ca6fc91d",
        "message": {
          "id": "be27fde0-613f-4a3a-ba81-f826ca6fc91d",
          "author": {
            "role": "assistant",
            "name": null,
