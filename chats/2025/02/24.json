      "205d3525-0346-4e30-928e-be79df30abba": {
        "id": "205d3525-0346-4e30-928e-be79df30abba",
        "message": {
          "id": "205d3525-0346-4e30-928e-be79df30abba",
          "author": {
            "role": "assistant",
